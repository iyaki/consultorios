{
  "name": "consultorios/orm",
  "version": "0.0.0",
  "require": {
    "php": "~8.1",
    "ext-pdo_mysql": "*",
    "consultorios/unit-of-work": "*",
    "doctrine/dbal": "^3.3",
    "doctrine/migrations": "^3.5",
    "doctrine/orm": "^2.12",
    "doctrine/persistence": "^3.0",
    "symfony/cache": "^6.1"
  },
  "require-dev": {
    "consultorios/dev-tools-settings": "*"
  },
  "autoload": {
    "psr-4": {
      "Consultorios\\ORM\\": "src"
    },
    "files": [
      "cli-config.php",
      "migrations.php"
    ]
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "path",
      "url": "../*",
      "only": ["consultorios/*"],
      "options": {
        "symlink": true
      }
    }
  ]
}
