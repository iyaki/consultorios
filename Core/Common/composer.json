{
  "name": "consultorios/core-common",
  "version": "0.0.0",
  "require": {
    "php": ">=8.0",
    "ext-pdo_mysql": "*",
    "consultorios/unit-of-work": "*",
    "doctrine/dbal": "^2",
    "doctrine/migrations": "^3.2",
    "doctrine/orm": "^2.9",
    "doctrine/persistence": "^2.2",
    "ramsey/uuid": "^4.1",
    "symfony/cache": "^5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "psalm/plugin-phpunit": "^0.16.1",
    "rector/rector": "^0.11.20",
    "symfony/var-dumper": "^5.3",
    "symplify/easy-coding-standard": "^9.3",
    "vimeo/psalm": "^4.9"
  },
  "autoload": {
    "psr-4": {
      "Consultorios\\Core\\Common\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../packages/php/*",
      "options": {
        "symlink": true
      }
    }
  ]
}
