{
  "name": "consultorios/core-common",
  "version": "0.0.0",
  "require": {
    "php": "~8.1"
  },
  "require-dev": {
    "consultorios/dev-tools-settings": "*"
  },
  "autoload": {
    "psr-4": {
      "Consultorios\\Core\\Common\\": "src"
    }
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../packages/php/*",
      "only": ["consultorios/*"],
      "options": {
        "symlink": true
      }
    }
  ]
}
