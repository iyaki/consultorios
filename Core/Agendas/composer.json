{
  "name": "consultorios/core-agendas",
  "version": "0.0.0",
  "require": {
    "php": ">=8.1",
    "consultorios/core-common": "*",
    "consultorios/orm": "*",
    "consultorios/unit-of-work": "*",
    "doctrine/orm": "^2.9",
    "ramsey/uuid": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "consultorios/dev-tools-settings": "*"
  },
  "autoload": {
    "psr-4": {
      "Consultorios\\Core\\Agendas\\": "src"
    },
    "exclude-from-classmap": ["*Test.php"]
  },
  "config": {
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../packages/php/*",
      "only": ["consultorios/*"],
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "../Common",
      "only": ["consultorios/core-common"],
      "options": {
        "symlink": true
      }
    }
  ]
}
