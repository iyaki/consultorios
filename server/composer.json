{
  "require": {
    "php": ">=8.0",
    "ext-intl": "*",
    "doctrine/orm": "^2.9",
    "laminas/laminas-diactoros": "^2.6",
    "laminas/laminas-servicemanager": "^3.6",
    "mezzio/mezzio": "^3.5",
    "mezzio/mezzio-cors": "^1.0",
    "mezzio/mezzio-fastroute": "^3.2",
    "mezzio/mezzio-helpers": "^5.6",
    "symfony/cache": "^5.3"
  },
  "require-dev": {
    "rector/rector": "^0.11.20",
    "symplify/easy-coding-standard": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "Consultorio\\": "app"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ComposerScripts\\": "tools/composer-scripts"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "check-style": "ComposerScripts\\StyleConventios::check",
    "check-style-precommit": "ComposerScripts\\StyleConventios::checkPreCommit",
    "fix-style": "ComposerScripts\\StyleConventios::fix",
    "check-types": "ComposerScripts\\StaticAnalysis::check",
    "check-types-precommit": "ComposerScripts\\StaticAnalysis::checkPreCommit",
    "fix-types": "ComposerScripts\\StaticAnalysis::fix",
    "refactor-dry": "ComposerScripts\\Refactor::dry",
    "refactor-postcommit": "ComposerScripts\\Refactor::postcommit",
    "refactor": "ComposerScripts\\Refactor::run"
  }
}
